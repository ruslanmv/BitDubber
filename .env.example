# BitDubber Environment Configuration Template
# Copy this file to .env and fill in your actual credentials

# IBM Watson Speech-to-Text Service
STT_API_KEY=your_speech_to_text_api_key_here
STT_URL=your_speech_to_text_service_url_here

# IBM Watson Text-to-Speech Service
TTS_API_KEY=your_text_to_speech_api_key_here
TTS_URL=your_text_to_speech_service_url_here

# IBM WatsonX (for LLaMA Models)
WATSONX_APIKEY=your_watsonx_api_key_here
WATSONX_URL=https://eu-de.ml.cloud.ibm.com
PROJECT_ID=your_project_id_here

# Optional: Logging Level (INFO, DEBUG, WARNING, ERROR, CRITICAL)
# LOG_LEVEL=INFO
